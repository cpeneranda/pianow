<div class="banner">
  <div class="video-bg">
    <iframe id="my-video" frameborder="0" height="100%" width="100%"
    src="https://youtube.com/embed/yUJwC7INGNo?autoplay=1&controls=0&showinfo=0&autohide=1&loop=1">
  </iframe>
</div>
<div class="banner-content">
    <h1>Pianow</h1>
    <p>Rent a pia<strong>now</strong></p>
    <div class="container">
      <div class="row">
        <div class="col-md-6 col-sm-offset-3">
          <form action="/pianos" method="get">
            <div id="custom-search-input">
              <%= render 'search-form' %>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>

<%= content_for(:after_js) do %>
<script src="https://maps.google.com/maps/api/js?libraries=places&amp;key=#{ENV['GOOGLE_API_BROWSER_KEY']}"></script>
<script async src="https://www.youtube.com/iframe_api"></script>

<% end %>

