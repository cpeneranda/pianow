<div class="banner">
<div class="video-bg">
    <iframe frameborder="0" height="100%" width="100%"
    src="https://youtube.com/embed/yUJwC7INGNo?autoplay=1&controls=0&showinfo=0&autohide=1">
  </iframe>
</div>
<div class="banner-content">
  <h1>Pianow</h1>
  <p>Rent a pia<strong>now</strong></p>
  <div class="container">
    <div class="row">
      <div class="col-md-6 col-sm-offset-3">
        <div id="custom-search-input">
          <div class="input-group col-md-12">
            <input type="text" class="form-control input-lg" placeholder="Paris..." id="search_place"/>
            <span class="input-group-btn">
              <button class="btn btn-info btn-lg" type="button">
                <i class="glyphicon glyphicon-search" style="color: #00CC7E"></i>
              </button>
            </span>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
</div>

<%= content_for(:after_js) do %>
<script src="https://maps.google.com/maps/api/js?libraries=places&amp;key=#{ENV['GOOGLE_API_BROWSER_KEY']}"></script>
<% end %>
